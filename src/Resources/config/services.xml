<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>

        <service id="Asam\FastOrderSW6\Core\Content\Product\SalesChannel\ProductSearch\ProductSearchRoute" public="true">
            <argument type="service" id="product.repository"/>
        </service>

        <service id="Asam\FastOrderSW6\Storefront\Page\FastOrder\FastOrderPageLoader" public="true">
            <argument type="service" id="Shopware\Storefront\Page\GenericPageLoader" />
            <argument type="service" id="event_dispatcher"/>
        </service>

        <service id="Asam\FastOrderSW6\Storefront\Controller\FastOrderController" public="true">
            <argument type="service" id="Asam\FastOrderSW6\Storefront\Page\FastOrder\FastOrderPageLoader" />
            <argument type="service" id="Asam\FastOrderSW6\Core\Content\Product\SalesChannel\ProductSearch\ProductSearchRoute" />
            <argument type="service" id="Shopware\Core\Checkout\Cart\SalesChannel\CartService"/>
            <argument type="service" id="Shopware\Core\Checkout\Cart\LineItemFactoryRegistry"/>
            <argument type="service" id="Shopware\Core\System\SystemConfig\SystemConfigService"/>
            <call method="setTwig">
                <argument type="service" id="twig"/>
            </call>
            <call method="setContainer">
                <argument type="service" id="service_container"/>
            </call>
        </service>

    </services>
</container>